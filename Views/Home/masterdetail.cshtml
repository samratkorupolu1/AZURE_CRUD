<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta http-equiv="X-UA-Compatible" content="ie=edge">
	<link rel="stylesheet" type="text/css" href="./css/main.css">
	<title>Main</title>
</head>

<body>
	<div class="header">
		<h2white>
			FBI Crimes by Offense and Year: Graphs
		</h2white>
		<hr>
		<pWhite>
			Project by Dharmarakshit Harley, Samrat Korupolu and Michael Zeolla
		</pWhite>
		<div class="header-right">
			<a asp-area="" asp-controller="Home" asp-action="Index">Home</a>
			<a class="active" asp-area="" asp-controller="Home" asp-action="mainpage">Main Page</a>
			<a asp-area="" asp-controller="Home" asp-action="aboutUs">About Us</a>
		</div>
	</div>
	<div class="container">
		@model IEnumerable<Assignment4.Models.EF_Models.ORI>
		@*@model Assignment4.Models.EF_Models.Rootobject

			@{
				ViewData["Title"] = "Index";
			}*@

		<table class="table">
			<thead>
				<tr>
					<th class="align-middle" scope="col">ORI</th>
					<th class="align-middle" scope="col">County</th>
					@*<th class="align-middle" scope="col">Total Count of Crimes</th>*@
					<th></th>
				</tr>
			</thead>
			<tbody>
				@foreach (Assignment4.Models.EF_Models.ORI row in Model)
				{
					<tr>
						<td>
							@row.nameORI
						</td>
						<td>
							@row.county
						</td>
						@*<td>
								@row.actual
							</td>*@
						@*<td>
								<form asp-controller="Home" asp-action="Remove" method="post">
									<input type="hidden" name="id" value="@row.nameORI" />
									<div>DEBUG: row.ID=@row.nameORI, row.actual=@row.county</div>
									<button type="submit">Delete</button>
								</form>
							</td>*@
					</tr>
				}
			</tbody>
		</table>

	</div>
	<div class="bottom">
		<a asp-area="" asp-controller="Home" asp-action="create"><input type=button value='CREATE'></a>
		<a asp-area="" asp-controller="Home" asp-action="update"><input type=button value='UPDATE'></a>
		<a asp-area="" asp-controller="Home" asp-action="delete"><input type=button value='DELETE'></a>
	</div>
	<div class="footer">
		<pWhite>Copyright &#169 May 2021 | ISM 6225 | USF</pWhite>
	</div>

</body>
</html>
