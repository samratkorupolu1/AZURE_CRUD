@model Assignment4.Models.EF_Models.Result

<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta http-equiv="X-UA-Compatible" content="ie=edge">
	<link rel="stylesheet" type="text/css" href="./css/main.css">
	<title>Main</title>
</head>

<body>
	<div class="header">
		<h2white>
			FBI Crimes by Offense and Year: Graphs
		</h2white>
		<hr>
		<pWhite>
			Project by Dharmarakshit Harley, Samrat Korupolu and Michael Zeolla
		</pWhite>
		<div class="header-right">

		</div>
	</div>
	<div class="container">
		<h1>update</h1>

		<h4>Result1</h4>
		<hr />
		<div class="row">
			<div class="col-md-4">
				<form asp-action="update">
					<div asp-validation-summary="ModelOnly" class="text-danger"></div>
					<div class="form-group" hidden>
						<label asp-for="ID" class="control-label"></label>
						<input asp-for="ID" class="form-control" />
						<span asp-validation-for="ID" class="text-danger"></span>
					</div>
					<div class="form-group" hidden>
						<label asp-for="ori" class="control-label"></label>
						<input asp-for="ori" class="form-control" />
						<span asp-validation-for="ori" class="text-danger"></span>
					</div>
					<div class="form-group" hidden>
						<label asp-for="data_year" class="control-label"></label>
						<input asp-for="data_year" class="form-control" />
						<span asp-validation-for="data_year" class="text-danger"></span>
					</div>
					<div class="form-group" hidden>
						<label asp-for="offense" class="control-label"></label>
						<input asp-for="offense" class="form-control" />
						<span asp-validation-for="offense" class="text-danger"></span>
					</div>
					<div class="form-group" hidden>
						<label asp-for="state_abbr" class="control-label"></label>
						<input asp-for="state_abbr" class="form-control" />
						<span asp-validation-for="state_abbr" class="text-danger"></span>
					</div>
					<div class="form-group" hidden>
						<label asp-for="cleared" class="control-label"></label>
						<input asp-for="cleared" class="form-control" />
						<span asp-validation-for="cleared" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="actual" class="control-label"></label>
						<input asp-for="actual" class="form-control" />
						<span asp-validation-for="actual" class="text-danger"></span>
					</div>
					<div class="form-group">
						<input type="submit" value="Save" class="btn btn-primary" />
					</div>
				</form>
			</div>
		</div>

		<div>
			<a asp-action="Index">Back to List</a>
		</div>
	</div>
	<div class="footer">
		<pWhite>Copyright &#169 May 2021 | ISM 6225 | USF</pWhite>
	</div>
</body>
</html>
